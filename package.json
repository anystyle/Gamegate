{
  "name": "gamegate",
  "version": "1.0.0",
  "description": "轻量级解压小游戏聚合平台 - 即开即玩、无负担休闲",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server.js",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "webpack --mode production",
    "test": "jest",
    "lint": "eslint src/",
    "preview": "vite preview"
  },
  "keywords": [
    "小游戏",
    "休闲游戏",
    "在线游戏",
    "即开即玩",
    "减压游戏",
    "摸鱼",
    "小游戏聚合",
    "网页游戏"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^4.1.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "terser": "^5.19.2",
    "vite": "^4.4.9",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
